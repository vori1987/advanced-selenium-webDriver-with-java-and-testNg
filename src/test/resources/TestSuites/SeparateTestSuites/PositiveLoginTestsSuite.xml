<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="PositiveLoginTestSuite" verbose="1" parallel="tests" thread-count="4">

    <test name="PositiveLoginTests-WithProfile">
        <parameter name="chromeProfile" value="ChromeProfile"/>
        <classes>
            <class name="com.herokuapp.theinternet.loginpagetest.PositiveLogInTests"/>
        </classes>
    </test>

    <test name="PositiveLoginTests-WithoutProfile">
        <classes>
            <class name="com.herokuapp.theinternet.loginpagetest.PositiveLogInTests"/>
        </classes>
    </test>


</suite>
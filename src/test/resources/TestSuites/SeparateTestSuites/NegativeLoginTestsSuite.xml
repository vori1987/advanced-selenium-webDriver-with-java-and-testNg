<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NegativeLoginTestSuite" verbose="1" parallel="tests" thread-count="2">

    <listeners>
        <listener class-name="com.herokuapp.theinternet.base.TestListener"></listener>
    </listeners>

    <test name="NegativeLogInTest-CH">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.herokuapp.theinternet.loginpagetest.NegativeLogInTests">
            </class>
        </classes>

    </test>
    <test name="NegativeLogInTest-FF">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.herokuapp.theinternet.loginpagetest.NegativeLogInTests">
            </class>
        </classes>
    </test>

</suite>